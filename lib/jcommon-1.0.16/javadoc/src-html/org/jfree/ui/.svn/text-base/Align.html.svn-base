<FONT color="green">121</FONT>        <a name="line.121"></a>
<FONT color="green">122</FONT>        /**<a name="line.122"></a>
<FONT color="green">123</FONT>         * Aligns one rectangle (&lt;code&gt;rect&lt;/code&gt;) relative to another rectangle (&lt;code&gt;frame&lt;/code&gt;).<a name="line.123"></a>
<FONT color="green">124</FONT>         *<a name="line.124"></a>
<FONT color="green">125</FONT>         * @param rect  the rectangle to be aligned (&lt;code&gt;null&lt;/code&gt; not permitted).<a name="line.125"></a>
<FONT color="green">126</FONT>         * @param frame  the reference frame (&lt;code&gt;null&lt;/code&gt; not permitted).<a name="line.126"></a>
<FONT color="green">127</FONT>         * @param align  the alignment code.<a name="line.127"></a>
<FONT color="green">128</FONT>         */<a name="line.128"></a>
<FONT color="green">129</FONT>        public static void align(final Rectangle2D rect, final Rectangle2D frame, final int align) {<a name="line.129"></a>
<FONT color="green">130</FONT>    <a name="line.130"></a>
<FONT color="green">131</FONT>            double x = frame.getCenterX() - rect.getWidth() / 2.0;<a name="line.131"></a>
<FONT color="green">132</FONT>            double y = frame.getCenterY() - rect.getHeight() / 2.0;<a name="line.132"></a>
<FONT color="green">133</FONT>            double w = rect.getWidth();<a name="line.133"></a>
<FONT color="green">134</FONT>            double h = rect.getHeight();<a name="line.134"></a>
<FONT color="green">135</FONT>    <a name="line.135"></a>
<FONT color="green">136</FONT>            if ((align &amp; FIT_VERTICAL) == FIT_VERTICAL) {<a name="line.136"></a>
<FONT color="green">137</FONT>                h = frame.getHeight();<a name="line.137"></a>
<FONT color="green">138</FONT>            }<a name="line.138"></a>
<FONT color="green">139</FONT>    <a name="line.139"></a>
<FONT color="green">140</FONT>            if ((align &amp; FIT_HORIZONTAL) == FIT_HORIZONTAL) {<a name="line.140"></a>
<FONT color="green">141</FONT>                w = frame.getWidth();<a name="line.141"></a>
<FONT color="green">142</FONT>            }<a name="line.142"></a>
<FONT color="green">143</FONT>    <a name="line.143"></a>
<FONT color="green">144</FONT>            if ((align &amp; TOP) == TOP) {<a name="line.144"></a>
<FONT color="green">145</FONT>                y = frame.getMinY();<a name="line.145"></a>
<FONT color="green">146</FONT>            }<a name="line.146"></a>
<FONT color="green">147</FONT>    <a name="line.147"></a>
<FONT color="green">148</FONT>            if ((align &amp; BOTTOM) == BOTTOM) {<a name="line.148"></a>
<FONT color="green">149</FONT>                y = frame.getMaxY() - h;<a name="line.149"></a>
<FONT color="green">150</FONT>            }<a name="line.150"></a>
<FONT color="green">151</FONT>    <a name="line.151"></a>
<FONT color="green">152</FONT>            if ((align &amp; LEFT) == LEFT) {<a name="line.152"></a>
<FONT color="green">153</FONT>                x = frame.getX();<a name="line.153"></a>
<FONT color="green">154</FONT>            }<a name="line.154"></a>
<FONT color="green">155</FONT>    <a name="line.155"></a>
<FONT color="green">156</FONT>            if ((align &amp; RIGHT) == RIGHT) {<a name="line.156"></a>
<FONT color="green">157</FONT>                x = frame.getMaxX() - w;<a name="line.157"></a>
<FONT color="green">158</FONT>            }<a name="line.158"></a>
<FONT color="green">159</FONT>    <a name="line.159"></a>
<FONT color="green">160</FONT>            rect.setRect(x, y, w, h);<a name="line.160"></a>
